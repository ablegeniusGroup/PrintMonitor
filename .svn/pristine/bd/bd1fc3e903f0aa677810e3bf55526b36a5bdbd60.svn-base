.flowPane{
    -fx-background-color: #34495E;
    -fx-alignment: center;
    -fx-border-color: white;
    -fx-border-width: 3px;
}
.contentLabel{
    -fx-background-color: #EBEBEB;
    -fx-alignment: center;
    -fx-font-weight: bolder;
    -fx-font-size: 25px;
    -fx-text-fill: red;
}
.titleLabel{
    -fx-background-color: #34495E;
    -fx-alignment: center;
    -fx-font-weight: bolder;
    -fx-font-size: 35px;
    -fx-text-fill:white;
}
.buttomFlowPane{
    -fx-background-color: #34495E;
    -fx-alignment: center;
}
.button{
    -fx-background-color: #5CAA88;
    -fx-alignment: center;
    -fx-font-weight: bolder;
    -fx-font-size: 25px;
    -fx-border-color: white;
    -fx-border-width: 1px;
    -fx-text-fill: white;
}



